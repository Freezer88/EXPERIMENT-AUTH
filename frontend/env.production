# Frontend Production Environment Configuration
VITE_NODE_ENV=production

# API Configuration
VITE_API_BASE_URL=https://your-production-domain.com/api
VITE_WS_BASE_URL=wss://your-production-domain.com

# OpenAI Configuration
VITE_OPENAI_MODEL=gpt-4-vision-preview

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_live_key_here

# Feature Flags
VITE_ENABLE_AI_PROCESSING=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_PAYMENT_PROCESSING=true
VITE_ENABLE_EXPORT_FUNCTIONALITY=true

# Analytics Configuration
VITE_ANALYTICS_ENABLED=true
VITE_SENTRY_DSN=your-production-sentry-dsn

# Development Tools
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/heic,application/pdf

# AI Processing Configuration
VITE_AI_PROCESSING_TIMEOUT=300000
VITE_AI_MAX_RETRIES=3

# UI Configuration
VITE_APP_NAME=Homeowners Insurance AI
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered inventory management and insurance analysis

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_PWA=true
VITE_CACHE_STRATEGY=network-first 